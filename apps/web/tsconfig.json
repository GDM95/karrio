{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "paths": {
      "@site/*": ["./*"],
      "@components/*": ["./src/components/*"],
      "@theme/*": ["./src/theme/*"]
    },
    "jsx": "react-jsx",
    "skipLibCheck": true
  },
  "include": ["src/", "docusaurus.config.ts", "sidebars.ts"]
}
